    </referenceResource>
  </binding>
  <binding>
    <name value="MediaSubtype"/>
    <isExtensible value="true"/>
    <conformance value="example"/>
    <referenceResource>
      <type value="ValueSet"/>
      <reference value="http://hl7.org/fhir/vs/media-subtype"/>
    </referenceResource>
  </binding>
  <binding>
    <name value="MediaView"/>
    <isExtensible value="true"/>
    <conformance value="example"/>
    <referenceResource>
      <type value="ValueSet"/>
      <reference value="http://hl7.org/fhir/vs/media-view"/>
    </referenceResource>
  </binding>
</Profile>