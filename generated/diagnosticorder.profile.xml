  &lt;<a title="The status for the event" class="dict" href="diagnosticorder-definitions.htm#DiagnosticOrder.event.status"><b>status</b></a> value=&quot;[<span style="color: darkgreen"><a href="datatypes.htm#code">code</a></span>]&quot;/&gt;<span style="color: Gray">&lt;!--</span> <span style="color: brown"><b>1..1</b></span> <span style="color: navy"><a style="color: navy" href="diagnostic-order-status.htm">requested | received | accepted | inprogress | review | complete | suspended | rejected | failed</a></span><span style="color: Gray"> --&gt;</span>
  &lt;<a title="The date/time at which the event occurred" class="dict" href="diagnosticorder-definitions.htm#DiagnosticOrder.event.date"><b>date</b></a> value=&quot;[<span style="color: darkgreen"><a href="datatypes.htm#dateTime">dateTime</a></span>]&quot;/&gt;<span style="color: Gray">&lt;!--</span> <span style="color: brown"><b>1..1</b></span> <span style="color: navy">The date at which the event happened</span><span style="color: Gray"> --&gt;</span>
