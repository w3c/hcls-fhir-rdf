   &lt;<a title="The name of a parameter" class="dict" href="documentreference-definitions.htm#DocumentReference.service.parameter.name"><b>name</b></a> value=&quot;[<span style="color: darkgreen"><a href="datatypes.htm#string">string</a></span>]&quot;/&gt;<span style="color: Gray">&lt;!--</span> <span style="color: brown"><b>1..1</b></span> <span style="color: navy">Name of parameter</span><span style="color: Gray"> --&gt;</span>
