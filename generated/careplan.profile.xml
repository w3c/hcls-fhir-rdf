        <max value="1"/>
        <type>
          <code value="code"/>
        </type>
        <mustUnderstand value="false"/>
        <binding value="CarePlanActivityStatus"/>
        <mapping>
          <target value="http://hl7.org/v2"/>
          <map value="ORC-5-order status?"/>
