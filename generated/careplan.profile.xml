        <mustUnderstand value="false"/>
        <mapping>
          <target value="http://hl7.org/v2"/>
          <map value="OBR-24-diagnostic service section ID (???!!)"/>
        </mapping>
      </definition>
    </element>
    <element>
      <path value="CarePlan.activity.performer"/>
      <definition>
        <short value="Who&#39;s responsible?"/>
        <formal value="Identifies who&#39;s expected to be involved in the activity."/>
