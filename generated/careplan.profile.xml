        <max value="1"/>
        <type>
          <code value="CodeableConcept"/>
        </type>
        <mustUnderstand value="false"/>
        <binding value="CarePlanActivityCode"/>
        <mapping>
          <target value="http://hl7.org/v2"/>
          <map value="OBR-4-universal service ID / RXE-2-Give Code / RXO-1-Requested Give Code / RXD-2-Dispense/Give Code"/>
